import * as utilAction from '../util';
//增加活动次数
export function addAmount(form) {
    window.ajax.post(window.config.root + '/event_manage/event_count', form, function (ret) {
        if (ret.errcode) {
            utilAction.prompt(ret.msg);
        } else {
            utilAction.prompt(ret.msg);
        }
    });
}
